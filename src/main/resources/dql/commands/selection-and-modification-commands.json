{
  "fields": {
    "name": "fields",
    "description": "Keeps only specified fields.",
    "type": "selection-and-modification-commands",
    "syntax": [
      "fields fieldName [= Expression] [, ...]"
    ],
    "parameters": [
      {
        "name": "field",
        "nameAllowed": false,
        "required": true,
        "repetitive": true,
        "description": "The definition of the fields for the records to be returned",
        "type": [
          "data object",
          "string",
          "number",
          "duration",
          "boolean",
          "timestamp",
          "uid",
          "ip",
          "counter",
          "binary",
          "timeframe",
          "array",
          "expression",
          "assign expression"
        ]
      }
    ]
  },
  "fieldsAdd": {
    "name": "fieldsAdd",
    "description": "Evaluates an expression and appends or replaces a field.",
    "type": "selection-and-modification-commands",
    "syntax": [
      "fieldsAdd fieldName [= Expression] [, ...]"
    ],
    "parameters": [
      {
        "name": "field",
        "nameAllowed": false,
        "required": true,
        "repetitive": true,
        "description": "The definition of the fields to be added to the returned records",
        "type": [
          "data object",
          "string",
          "number",
          "duration",
          "boolean",
          "timestamp",
          "uid",
          "ip",
          "counter",
          "binary",
          "timeframe",
          "array",
          "expression",
          "assign expression"
        ]
      }
    ]
  },
  "fieldsKeep": {
    "name": "fieldsKeep",
    "description": "Keeps selected fields.",
    "type": "selection-and-modification-commands",
    "syntax": [
      "fieldsKeep fieldNameOrPattern [,...]"
    ],
    "parameters": [
      {
        "name": "fields",
        "nameAllowed": false,
        "required": true,
        "repetitive": true,
        "description": "The definition of the fields for the records to be returned",
        "type": [
          "data object",
          "read-only expression"
        ]
      }
    ]
  },
  "fieldsRemove": {
    "name": "fieldsRemove",
    "description": "Removes fields from the result.",
    "type": "selection-and-modification-commands",
    "syntax": [
      "fieldsRemove fieldNameOrPattern [,...]"
    ],
    "parameters": [
      {
        "name": "field",
        "nameAllowed": false,
        "required": true,
        "repetitive": true,
        "description": "The definition of the fields to be removed from returned records",
        "type": [
          "data object",
          "read-only expression"
        ]
      }
    ]
  },
  "fieldsRename": {
    "name": "fieldsRename",
    "description": "Renames a field.",
    "type": "selection-and-modification-commands",
    "syntax": [
      "fieldsRename fieldName = originalName, [,...]"
    ],
    "parameters": [
      {
        "name": "field",
        "nameAllowed": false,
        "required": true,
        "repetitive": true,
        "description": "The redefinition of the fields names for the records to be returned",
        "type": [
          "write-only expression"
        ]
      }
    ]
  }
}